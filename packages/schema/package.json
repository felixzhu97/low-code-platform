{
  "name": "@lowcode-platform/schema",
  "version": "0.1.0",
  "type": "module",
  "description": "Schema validation, serialization, and migration utilities for low-code platform",
  "license": "MIT",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./types": "./src/types.ts",
    "./validator": "./src/validator.ts",
    "./serializer": "./src/serializer.ts",
    "./migrator": "./src/migrator.ts"
  },
  "files": [
    "src/**/*"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@types/node": "^22",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
